<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>

    <Authors>egorozh</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/egorozh/Egorozh.Razor.Layout</PackageProjectUrl>
    <RepositoryUrl>https://github.com/egorozh/Egorozh.Razor.Layout</RepositoryUrl>
    <PackageTags>Razor, Blazor, Css, Layout, Grid, CSS Grid Layout</PackageTags>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <Description>Blazor (Razor) layout based on CSS grid</Description>

    <Version>5.0.3</Version>
    <AssemblyVersion>5.0.3</AssemblyVersion>
    <FileVersion>5.0.3</FileVersion>
  </PropertyGroup>
 
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="../LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>